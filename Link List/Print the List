/* struct Node
{
    int data;
    struct Node* next;
};
*/
void forwardPrint(struct Node* head)
{
	struct Node *temp = head;
 	while(temp != NULL)
    {
    printf("%d-",temp -> data);
    temp = temp -> next;
    }
}

void backwardPrint(struct Node* head)
{
  int arr[5],i=0;
  struct Node *temp = head;
 	while(temp != NULL)
    {
    arr[i]=temp -> data;
     i++;
    temp = temp -> next;
    }
  	i=i-1;
  	for(i;i>=0;i--){
    printf("%d-",arr[i]);
  }
}
